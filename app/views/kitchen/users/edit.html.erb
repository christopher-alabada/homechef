<div class="explore">
  <h1>Edit Profile</h1>
</div>

<br>

<div class="container">
  <%= simple_form_for @foodie, url: kitchen_update_foodie_path do |f| %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :address %>
    <%= f.input :photo %>
    <%= f.submit "Edit Profile", class: 'btn btn-success' %>
  <% end %>
</div>

